{
  "name": "ember-junkdrawer",
  "version": "0.13.1",
  "description": "Utility belt of bootstrap extras.",
  "keywords": [
    "ember-addon"
  ],
  "repository": "mwisner/ember-junkdrawer",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "ember test",
    "test:all": "ember try:each",
    "lint:hbs": "ember-template-lint .",
    "nodetest": "mocha node-tests --recursive"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ],
    "*.scss": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "broccoli-debug": "^0.6.3",
    "broccoli-funnel": "^2.0.1",
    "broccoli-merge-trees": "^2.0.0",
    "ember-cli-babel": "^7.1.2",
    "ember-cli-htmlbars": "^3.0.0"
  },
  "devDependencies": {
    "@ember/optional-features": "^0.6.3",
    "bootstrap": "4.0.0-beta.2",
    "broccoli-asset-rev": "^2.7.0",
    "ember-bootstrap": "^1.0.0-rc.5",
    "ember-changeset": "^1.3.0",
    "ember-changeset-validations": "^1.2.8",
    "ember-cli": "~3.9.0",
    "ember-cli-addon-docs": "^0.6.0",
    "ember-cli-addon-docs-yuidoc": "^0.2.1",
    "ember-cli-dependency-checker": "^3.1.0",
    "ember-cli-deploy": "^1.0.2",
    "ember-cli-deploy-build": "^1.1.1",
    "ember-cli-deploy-git": "^1.3.2",
    "ember-cli-deploy-git-ci": "^1.0.1",
    "ember-cli-eslint": "^4.2.3",
    "ember-cli-flash": "^1.6.3",
    "ember-cli-htmlbars-inline-precompile": "^1.0.3",
    "ember-cli-inject-live-reload": "^1.8.2",
    "ember-cli-mirage": "^0.4.14",
    "ember-cli-sass": "^7.1.4",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-uglify": "^2.1.0",
    "ember-cli-update": "^0.27.3",
    "ember-concurrency": "^0.10.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.0",
    "ember-file-upload": "^2.2.4",
    "ember-load-initializers": "^1.1.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-native-dom-helpers": "^0.6.2",
    "ember-qunit": "^3.4.1",
    "ember-resolver": "^5.0.1",
    "ember-source": "~3.9.0",
    "ember-source-channel-url": "^1.1.0",
    "ember-test-free-g": "^0.0.2",
    "ember-toggle": "^5.2.1",
    "ember-try": "^1.0.0",
    "eslint-plugin-ember": "^5.2.0",
    "eslint-plugin-node": "^7.0.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.3",
    "loader.js": "^4.7.0",
    "prettier": "^1.16.4",
    "qunit-dom": "^0.8.0",
    "mocha": "^6.0.0"
  },
  "engines": {
    "node": "6.* || 8.* || >= 10.*"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://mwisner.github.io/ember-junkdrawer/"
  }
